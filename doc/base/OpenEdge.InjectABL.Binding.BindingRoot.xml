    <method methodName="Bind" returnType="IBindingSyntax" signature="Bind(INPUT:Progress.Lang.Class):IBindingSyntax" modifier="PUBLIC" isStatic="false" isFinal="false" isAbstract="false">
        <comment>/** Declares a binding from the service to itself.        &#xD;
        @param Class The service to bind. */</comment>
        <parameter name="poService" dataType="Progress.Lang.Class" position="0" mode="INPUT"/>
    </method>
    <method methodName="Unbind" signature="Unbind(INPUT:Progress.Lang.Class)" modifier="PUBLIC" isStatic="false" isFinal="false" isAbstract="false">
        <comment>/** Unregisters all bindings for the specified service.&#xD;
        &#xD;
        @param Class The service to unbind. */</comment>
        <parameter name="poService" dataType="Progress.Lang.Class" position="1" mode="INPUT"/>
    </method>
    <method methodName="Rebind" returnType="IBindingSyntax" signature="Rebind(INPUT:Progress.Lang.Class):IBindingSyntax" modifier="PUBLIC" isStatic="false" isFinal="false" isAbstract="false">
        <comment>/** Removes any existing bindings for the specified service, and declares a new one.&#xD;
        &#xD;
        @param Class The service to re-bind. **/</comment>
        <parameter name="poService" dataType="Progress.Lang.Class" position="2" mode="INPUT"/>
    </method>
    <method methodName="AddBinding" signature="AddBinding(INPUT:IBinding)" modifier="PUBLIC" isStatic="false" isFinal="false" isAbstract="true">
        <comment>/** Registers the specified binding.&#xD;
        &#xD;
        @param IBinding The binding to add. */</comment>
        <parameter name="poBinding" dataType="IBinding" position="3" mode="INPUT"/>
    </method>
    <method methodName="RemoveBinding" signature="RemoveBinding(INPUT:IBinding)" modifier="PUBLIC" isStatic="false" isFinal="false" isAbstract="true">
        <comment>/** Unregisters the specified binding.&#xD;
        @param IBinding The binding to remove. */</comment>
        <parameter name="poBinding" dataType="IBinding" position="4" mode="INPUT"/>
    </method>
    <method methodName="CreateBindingBuilder" returnType="IBindingSyntax" signature="CreateBindingBuilder(INPUT:IBinding):IBindingSyntax" modifier="PROTECTED" isStatic="false" isFinal="false" isAbstract="true">
        <parameter name="poBinding" dataType="IBinding" position="5" mode="INPUT"/>
    </method>
    <property name="Bindings" dataType="IMap" isAbstract="true" isStatic="false" extent="0" modifier="PUBLIC" getModifier="PUBLIC" setModifier="NONE">
        <propertyComment>/** A collection of bindings, keyed by the service they bind to. */</propertyComment>
    </property>
    <using name="OpenEdge.InjectABL.Binding.IBindingRoot" type="NONE"/>
